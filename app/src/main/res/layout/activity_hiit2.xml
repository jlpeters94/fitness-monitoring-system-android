<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_starting_strength"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/colorPrimary"
    tools:context="com.hehe.trumpcard.HIIT2">

    <TextView
        android:text="HIIT 2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView23"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:textColor="@android:color/white"
        android:textStyle="normal|bold"
        android:textSize="24sp" />

    <TextView
        android:text="Advanced Bodyweight Weight Loss Routine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView23"
        android:layout_alignParentStart="true"
        android:id="@+id/textView24"
        android:textColor="@android:color/white"
        android:textSize="18sp" />

    <TextView
        android:text="This is the part 2 of the HIIT program if you are already used to it and wanted to seek a more challenging workout program! Basically the rule is still the same. Do a movement for 30 seconds then move on to the next move. Then repeat the routine 3 times!\n\n  Push Ups - 30 seconds\n Squats - 30 seconds\n Burpees - 30 seconds\n High Knees - 30 seconds\n Rest 30 seconds\n\n  Tricep Dips - 30 seconds\n Lunges - 30 seconds\n Jumping Jacks - 30 seconds\n Stationary Jog - 30 seconds\n Rest 30 seconds\n\n  Bicycle Crunches - 30 seconds\n Plank - 30 seconds\n Mountain Climbers - 30 seconds\n Diamond Jumps - 30 seconds\n Rest 30 seconds\n\n  Repeat this 3 times then your are done!"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView24"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:id="@+id/textView25"
        android:textColor="@android:color/white"
        android:textSize="15sp" />

    <Button
        android:text="BACK"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/button35"
        android:background="@android:color/white"
        android:textSize="24sp"
        android:textColor="@color/colorPrimary"
        android:textStyle="normal|bold" />

</RelativeLayout>
