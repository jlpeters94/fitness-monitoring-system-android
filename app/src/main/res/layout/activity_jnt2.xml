<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_starting_strength"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/colorPrimary"
    tools:context="com.hehe.trumpcard.JNT2">

    <TextView
        android:text="JACKED AND TAN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView23"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:textColor="@android:color/white"
        android:textStyle="normal|bold"
        android:textSize="24sp" />

    <TextView
        android:text="Powerbuilding Program"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView23"
        android:layout_alignParentStart="true"
        android:id="@+id/textView24"
        android:textColor="@android:color/white"
        android:textSize="18sp" />

    <TextView
        android:text="Day 1\n Squat 5 x 3\n Close Grip Bench 3 x 10\n Heavy Shrugs 3 x 10\n Cable Row 3 x 15\n Lat Pulldown 3 x 15\n Abs 3 x 15\n\n  Day 2\n Bench 5 x 3\n Paused Squat 7 x 3\n Seated Dumbbell OHP 3 x 15\n Cable Row 3 x 15\n Rope Tricep Pushdown 3 x 15\n Face Pulls 3 x 15\n Leg Press 3 x 15\n\n  Day 3\n Deadlift 5 x 3\n Legs Up Bench 3 x 10\n Light Shrugs 3 x 15\n Cable Row 3 x 15\n Lat Pulldown 3 x 15\n Biceps Curls 3 x 15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView24"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:id="@+id/textView25"
        android:textColor="@android:color/white"
        android:textSize="15sp" />

    <Button
        android:text="BACK"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/button23"
        android:background="@android:color/white"
        android:textSize="24sp"
        android:textColor="@color/colorPrimary"
        android:textStyle="normal|bold" />

    <TextView
        android:text="Day 4\n OHP 5 x 3\n Paused Deadlift 7 x 3\n Incline Dumbbell Bench 3 x 15\n Cable Row 3 x 15\n Bar Tricep Pushdown 3 x 15\n Face Pulls 3 x 15\n Leg Press 3 x 15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView25"
        android:layout_toEndOf="@+id/textView25"
        android:id="@+id/textView26"
        android:textSize="15sp"
        android:textColor="@android:color/white" />
</RelativeLayout>
